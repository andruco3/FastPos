                                             </children>
                                          </HBox>
                                          <HBox spacing="20.0">
                                             <children>
                                                <JFXComboBox fx:id="comboBoxType11" labelFloat="true" prefWidth="247.0" promptText="Key" />
                                                <JFXButton text="Manage Key" />
                                                <JFXToggleButton fx:id="toogleButtonField11" maxHeight="30.0" minHeight="30.0" onAction="#activeFields" text="Mac" />
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                 </content>
                              </TitledPane>
                              <TitledPane animated="false" text="Field">
                                 <content>
                                    <VBox spacing="15.0">
                                       <children>
                                          <HBox spacing="20.0">
                                             <children>
                                                <JFXToggleButton fx:id="toogleButtonField" maxHeight="30.0" minHeight="30.0" onAction="#activeFields" text="Fields" />
                                                <JFXComboBox fx:id="comboBoxSense" labelFloat="true" promptText="Sense" />
