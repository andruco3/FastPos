                                             </children>
                                          </HBox>
                                          <HBox spacing="20.0">
                                             <children>
                                                <JFXTextField fx:id="textFieldIPName1" labelFloat="true" prefWidth="120.0" promptText="TimeOut" />
                                                <JFXTextField fx:id="textFieldIPName11" labelFloat="true" prefWidth="120.0" promptText="Forward" />
                                                <JFXComboBox fx:id="comboBoxType2" labelFloat="true" prefWidth="247.0" promptText="Type" />
                                             </children>
                                          </HBox>
                                          <HBox spacing="20.0">
                                             <children>
                                                <JFXTextField fx:id="textFieldIP1" labelFloat="true" prefWidth="247.0" promptText="Header" />
